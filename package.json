{
  "name": "personalwebsite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "eject": "react-scripts eject",
    "build": "next build  && next export",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(js)\"",
    "prettier": "pretty-quick --staged --ignore-path .gitignore \"**/*.+(js)\"",
    "format": "yarn prettier -- --write",
    "check-format": "yarn prettier -- --list-different",
    "validate": "yarn format && git add . && npm-run-all --parallel check-format lint"
  },
  "dependencies": {
    "husky": "4.2.5",
    "next": "10.0.7",
    "npm-run-all": "^4.1.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
